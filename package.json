{
  "name": "grafana-lokiexplore-app",
  "version": "1.0.15",
  "description": "Query less exploration of log data stored in Loki",
  "scripts": {
    "build": "webpack -c ./webpack.config.ts --env production",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "yarn run lint --fix && prettier --write --list-different .",
    "e2e": "playwright test -x --trace on",
    "e2e:fast": "playwright test --retries=1 --workers=3",
    "e2e:10x": "playwright test -x --trace on --repeat-each=10 --workers 1",
    "e2e:10x:fast": "playwright test --repeat-each=10 --workers 3 --retries=1",
    "e2e:flake": "yarn e2e:10x",
    "e2e:debug": "playwright test --debug -x --trace on /test",
    "e2e:codegen": "playwright install && playwright codegen",
    "generate-logs": "pushd generator; docker build -t log-generator . && docker run log-generator -url http://host.docker.internal:3100/loki/api/v1/push; popd",
    "server": "docker compose -f docker-compose.local.yaml up --build",
    "start": "yarn build && docker compose -f docker-compose.local.yaml up --build -d && yarn dev",
    "server:localLoki": "docker compose -f docker-compose-local-loki.dev.yaml up --build",
    "server:ci": "docker compose -f docker-compose.dev.yaml up --build -d",
    "server:syslog": "docker compose -f docker-compose-syslog.dev.yaml up --build",
    "server:down": "docker compose -f docker-compose.local.yaml down",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "prepare": "husky",
    "docs": "cd docs && make docs",
    "spellcheck": "cspell -c cspell.config.json \"**/*.{ts,tsx,js,go,md,mdx,yml,yaml,json,scss,css}\""
  },
  "author": "Grafana",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@grafana/eslint-config": "^8.0.0",
    "@grafana/plugin-e2e": "^1.17.1",
    "@grafana/tsconfig": "^2.0.0",
    "@playwright/test": "^1.52.0",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@swc/core": "^1.4.2",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",
    "@types/react-scroll-sync": "^0.9.0",
    "@types/react-table": "^7.7.20",
    "@types/testing-library__jest-dom": "5.14.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "copy-webpack-plugin": "^11.0.0",
    "cspell": "6.13.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^46.8.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort": "^4.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "husky": "^9.0.11",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "react-scroll-sync": "^0.11.2",
    "react-select-event": "^5.5.1",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "semver": "^7.6.3",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.10",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4",
    "webpack": "5.95.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@bsull/augurs": "^0.6.0",
    "@emotion/css": "11.10.6",
    "@grafana/data": "^11.6.1",
    "@grafana/lezer-logql": "^0.2.7",
    "@grafana/runtime": "^11.6.1",
    "@grafana/scenes": "^6.9.0",
    "@grafana/ui": "^11.6.1",
    "@gtk-grafana/react-json-tree": "^0.0.9",
    "@hello-pangea/dnd": "^16.6.0",
    "@lezer/common": "^1.2.1",
    "@lezer/lr": "^1.4.1",
    "@types/react-table": "^7.7.20",
    "re2js": "^1.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.22.0",
    "rxjs": "7.8.1",
    "tslib": "2.5.3",
    "uuid": "^10.0.0",
    "@grafana/schema": "^11.5.3"
  },
  "resolutions": {
    "react-router-dom": "^6.22.0"
  },
  "packageManager": "yarn@1.22.21"
}
